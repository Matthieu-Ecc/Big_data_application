name: big_data_application_sahnou_eccher

conda_env: conda.yaml

entry_points:
  main:
    parameters:
      random_state: {type: int, default: 5}
      lr: {type: float, default: 0.01}
      max_depth: {type: int, default: 10}
      n_estimator: {type: int, default: 100}
    command: "python train_xgb.py {random_state} {lr}"
    command: "python train_random_forest.py {max_depth}"
    command: "python train_gradient_boosting.py {n_estimator} {lr} {max_depth}"
